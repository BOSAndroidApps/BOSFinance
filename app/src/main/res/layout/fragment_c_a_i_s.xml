<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.slideshow.ui.view.activity.retailer.cibilreportsfragment.CAIS">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/_10sdp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">



        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=" Total Credit Accounts"
            android:fontFamily="@font/inter_medium"
            android:textColor="@color/darkpurple"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginHorizontal="@dimen/_10sdp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:background="@drawable/bg_black_border"
            android:gravity="center"
            android:layout_marginTop="@dimen/_2sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edittext">

            <TextView
                android:id="@+id/creditaccounts"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:hint="4"
                android:fontFamily="@font/inter_regular"
                android:inputType="text"
                android:imeOptions="actionNext"
                android:maxLength="12"
                android:padding="@dimen/_7sdp"
                android:textColor="@color/black"
                android:textSize="@dimen/_14ssp" />

        </LinearLayout>



        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Active Account"
            android:layout_marginTop="@dimen/_3sdp"
            android:fontFamily="@font/inter_medium"
            android:textColor="@color/darkpurple"
            android:layout_marginHorizontal="@dimen/_10sdp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:background="@drawable/bg_black_border"
            android:gravity="center"
            android:layout_marginTop="@dimen/_2sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edittext">

            <TextView
                android:id="@+id/activeaccount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:hint="2"
                android:fontFamily="@font/inter_regular"
                android:inputType="text"
                android:imeOptions="actionNext"
                android:maxLength="12"
                android:padding="@dimen/_7sdp"
                android:textColor="@color/black"
                android:textSize="@dimen/_14ssp" />

        </LinearLayout>



        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Default Acoount"
            android:layout_marginTop="@dimen/_3sdp"
            android:fontFamily="@font/inter_medium"
            android:textColor="@color/darkpurple"
            android:layout_marginHorizontal="@dimen/_10sdp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:background="@drawable/bg_black_border"
            android:gravity="center"
            android:layout_marginTop="@dimen/_2sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edittext">

            <TextView
                android:id="@+id/defaultaccount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:hint="1"
                android:fontFamily="@font/inter_regular"
                android:inputType="text"
                android:imeOptions="actionNext"
                android:maxLength="12"
                android:padding="@dimen/_7sdp"
                android:textColor="@color/black"
                android:textSize="@dimen/_14ssp" />

        </LinearLayout>



        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Closed Account"
            android:layout_marginTop="@dimen/_3sdp"
            android:fontFamily="@font/inter_medium"
            android:textColor="@color/darkpurple"
            android:layout_marginHorizontal="@dimen/_10sdp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:background="@drawable/bg_black_border"
            android:gravity="center"
            android:layout_marginTop="@dimen/_2sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edittext">

            <TextView
                android:id="@+id/closedaccount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:hint="2"
                android:fontFamily="@font/inter_regular"
                android:inputType="text"
                android:imeOptions="actionNext"
                android:maxLength="12"
                android:padding="@dimen/_7sdp"
                android:textColor="@color/black"
                android:textSize="@dimen/_14ssp" />

        </LinearLayout>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Total Outstanding Balance"
            android:layout_marginTop="@dimen/_3sdp"
            android:fontFamily="@font/inter_medium"
            android:textColor="@color/darkpurple"
            android:layout_marginHorizontal="@dimen/_10sdp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:background="@drawable/bg_black_border"
            android:gravity="center"
            android:layout_marginTop="@dimen/_2sdp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edittext">

            <TextView
                android:id="@+id/outstandingbalance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:hint="360262"
                android:fontFamily="@font/inter_regular"
                android:inputType="text"
                android:imeOptions="actionNext"
                android:maxLength="12"
                android:padding="@dimen/_7sdp"
                android:textColor="@color/black"
                android:textSize="@dimen/_14ssp" />

        </LinearLayout>


    </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>